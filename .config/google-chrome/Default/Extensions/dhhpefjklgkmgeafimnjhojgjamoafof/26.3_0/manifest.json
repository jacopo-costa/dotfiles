{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "icon16.png"
      },
      "default_title": "Save Page WE"
   },
   "commands": {
      "_execute_browser_action": {
         "description": "Save Page",
         "suggested_key": {
            "default": "Alt+A"
         }
      },
      "cancelsave": {
         "description": "Cancel Save",
         "suggested_key": {
            "default": "Alt+C"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content-fontface.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*" ],
      "run_at": "document_start"
   } ],
   "description": "Save a complete web page (as curently displayed) as a single HTML file that can be opened in any browser.",
   "icons": {
      "128": "icon128-96.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "64": "icon64.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "56",
   "name": "Save Page WE",
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "http://*/*", "https://*/*", "file:///*", "tabs", "webNavigation", "webRequest", "webRequestBlocking", "downloads", "contextMenus", "notifications", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "26.3",
   "web_accessible_resources": [ "lazyload-panel.html", "message-panel.html", "unsaved-panel.html", "comments-panel.html", "pageinfo-panel.html", "pageinfo-bar-compressed.html", "shadowloader-compressed.js" ]
}
